normal <- function(n, m = 0, s = 1){
x <- rnorm(n = n, mean = m, sd = s)
x <- sort(x)
prob <- pnorm(p = x, mean = m, sd = s)
pdf <- dnorm(x = x, mean = m, sd = s)
cbind(x, prob, pdf)
}
normal(12)
normal <- function(n, m = 0, s = 1){
x <- rnorm(n = n, mean = m, sd = s)
x <- sort(x)
prob <- pnorm(q = x, mean = m, sd = s)
pdf <- dnorm(x = x, mean = m, sd = s)
cbind(x, prob, pdf)
}
